%Space state model
A=[0,1;2,3];
B=[0;1];
C=eye(2);
D=0;

%Desired poles
p=[-3,-5];

%Pole placement method
K=place(A,B,p); %u=-Kx

%Euler Method
delta_t = 0.001;
tf=10;
t = 0:delta_t:tf;
x = zeros(length(t));
x(:,1) = [1;-3];
u = zeros(length(t));
for i=1:length(t)-1
    u(i)=-K*x(i);
    x(i+1) = x(i)+delta_t*(A*x(i)+B*u(i));

    